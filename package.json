{
    "name": "bogie-web",
    "version": "0.0.1",
    "description": "The web client for Bogie",
    "main": "dist/server.js",
    "scripts": {
        "lint": "./node_modules/.bin/xo src/**/*.js",
        "postinstall": "./node_modules/.bin/gulp babel client --no-watch",
        "start": "node dist/server.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sup3bsc/bogie-web.git"
    },
    "keywords": [
        "Web",
        "Client"
    ],
    "author": "Sup3 Bsc",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/sup3bsc/bogie-web/issues"
    },
    "homepage": "https://github.com/sup3bsc/bogie-web#readme",
    "xo": {
        "esnext": true,
        "envs": [
            "browser",
            "commonjs"
        ],
        "globals": [
            "cssInJS"
        ],
        "plugins": [
            "react"
        ],
        "extends": [
            "xo-react"
        ],
        "space": 4,
        "rules": {
            "react/jsx-closing-bracket-location": 0,
            "camelcase": 1,
            "linebreak-style": [
                2,
                "windows"
            ]
        }
    },
    "dependencies": {
        "babel-cli": "^6.3.17",
        "babel-core": "^6.3.17",
        "babel-plugin-css-in-js": "^1.1.0",
        "babel-plugin-transform-es2015-destructuring": "^6.3.15",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.3.16",
        "babel-plugin-transform-es2015-parameters": "^6.3.26",
        "babel-plugin-transform-es2015-unicode-regex": "^6.3.13",
        "babel-preset-es2015": "^6.3.13",
        "babel-preset-react": "^6.3.13",
        "babel-preset-stage-0": "^6.3.13",
        "babel-relay-plugin": "^0.6.0",
        "babelify": "^7.2.0",
        "body-parser": "^1.14.2",
        "browser-sync": "^2.10.1",
        "browserify": "^12.0.1",
        "cookie-parser": "^1.4.0",
        "dot": "^1.0.3",
        "express": "^4.13.3",
        "fbjs": "^0.4.0",
        "gulp": "^3.9.0",
        "gulp-babel": "^6.1.1",
        "gulp-nodemon": "^2.0.6",
        "gulp-sourcemaps": "^1.6.0",
        "gulp-uglify": "^1.5.1",
        "gulp-util": "^3.0.7",
        "history": "^1.17.0",
        "isomorphic-fetch": "^2.2.0",
        "isomorphic-relay": "^0.4.0",
        "morgan": "^1.6.1",
        "react": "^0.14.3",
        "react-dom": "^0.14.3",
        "react-relay": "^0.6.0",
        "react-router": "^1.0.3",
        "vinyl-buffer": "^1.0.0",
        "vinyl-source-stream": "^1.1.0",
        "watchify": "^3.6.1"
    },
    "devDependencies": {
        "eslint": "^1.10.3",
        "eslint-config-xo": "^0.9.1",
        "eslint-config-xo-react": "^0.3.0",
        "eslint-plugin-react": "^3.14.0",
        "pre-commit": "^1.1.2",
        "xo": "^0.12.1"
    },
    "pre-commit": [
        "lint"
    ]
}
